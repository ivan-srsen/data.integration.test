# Snowflake Connection Details
# Copy this file to .env and fill in your Snowflake trial account details

# Your Snowflake account identifier (e.g., xy12345.us-east-1)
# Find this in your Snowflake trial email or at the URL: https://<account>.snowflakecomputing.com
SNOWFLAKE_ACCOUNT=your-account-identifier

# Snowflake username
SNOWFLAKE_USER=your-username

# Snowflake password
SNOWFLAKE_PASSWORD=your-password

# Snowflake role (default: ACCOUNTADMIN for trial accounts)
SNOWFLAKE_ROLE=ACCOUNTADMIN

# Snowflake warehouse (default: COMPUTE_WH)
SNOWFLAKE_WAREHOUSE=COMPUTE_WH

# Snowflake database name
SNOWFLAKE_DATABASE=your-database

# Snowflake schema (default: PUBLIC)
SNOWFLAKE_SCHEMA=PUBLIC

# Airflow UID (leave as default unless you encounter permission issues)
AIRFLOW_UID=50000
